{
  "tasks": {
    "start": "deno run -A --watch=static/,routes/ dev.ts",
    "build": "deno run -A dev.ts build",
    "preview": "deno run -A main.ts"
  },
  "compilerOptions": { "jsx": "react-jsx", "jsxImportSource": "preact" },
  "imports": {
    "$fresh/": "https://deno.land/x/fresh@1.6.0/",
    "std/": "https://deno.land/std@0.182.0/",
    "postgres": "https://deno.land/x/postgres@v0.17.0/mod.ts",
    "preact": "https://esm.sh/preact@10.19.2",
    "preact/": "https://esm.sh/preact@10.19.2/",
    "preact-render-to-string": "https://esm.sh/*preact-render-to-string@6.0.2",
    "preact-markdown": "https://esm.sh/react-markdown@7.1.2?alias=react:preact/compat",
    "remark-directive": "https://esm.sh/remark-directive@2.0.1",
    "remark-directive-rehype": "https://esm.sh/remark-directive-rehype@0.4.2",
    "remark-gfm": "https://esm.sh/remark-gfm@3.0.1",
    "twind": "https://esm.sh/twind@0.16.19",
    "twind-preset-tailwind": "https://esm.sh/@twind/preset-tailwind@1.1.4",
    "twind-preset-typography": "https://esm.sh/@twind/preset-typography@1.0.7",
    "photoswipe": "https://esm.sh/photoswipe@5.3.7",
    "photoswipe/": "https://esm.sh/photoswipe@5.3.7/",
    "d3": "https://esm.sh/d3@7.8.2",
    "ol/": "https://cdn.skypack.dev/ol@7.4.0/",
    "computed-types": "https://denoporter.sirjosh.workers.dev/v1/deno.land/x/computed_types@v1.11.2/src/index.ts",
    "preactify-markdown/": "https://raw.githubusercontent.com/Tims777/preactify-markdown/0.3.1/",
    "@preact/signals": "https://esm.sh/*@preact/signals@1.2.1",
    "@preact/signals-core": "https://esm.sh/*@preact/signals-core@1.5.0",
    "twind/": "https://esm.sh/twind@0.16.19/"
  },
  "lint": { "rules": { "tags": ["fresh", "recommended"] } },
  "exclude": ["**/_fresh/*"]
}
